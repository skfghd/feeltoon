{
  "name": "fairy-tale-firebase",
  "version": "1.0.0",
  "description": "Firebase deployment package for AI Fairy Tale Generation Platform",
  "main": "public/index.html",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.firebase.config.ts",
    "build": "vite build --config vite.firebase.config.ts",
    "preview": "vite preview --config vite.firebase.config.ts",
    "functions:build": "cd functions && npm run build",
    "functions:serve": "cd functions && npm run serve",
    "functions:deploy": "cd functions && npm run deploy",
    "deploy": "npm run build && npm run functions:build && firebase deploy",
    "emulators": "firebase emulators:start",
    "setup": "npm install && cd functions && npm install",
    "check": "tsc --noEmit"
  },
